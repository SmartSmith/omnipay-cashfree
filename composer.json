{
    "name": "smartpay/omnipay-cashfree",
    "description": "Cashfree PG Orders API (v2023-08-01) driver for Omnipay",
    "type": "library",
    "license": "MIT",
    "keywords": ["cashfree", "omnipay", "payment", "gateway"],
    "homepage": "https://github.com/SmartSmith/omnipay-cashfree",
    "require": {
        "php": "^8.2",
        "omnipay/common": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "omnipay/tests": "^4.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Cashfree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Cashfree\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
